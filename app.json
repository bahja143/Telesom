{
  "expo": {
    "name": "Telesom",
    "slug": "Telesom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#99d600"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icon.png",
        "backgroundColor": "#99d600"
      },
       "package": "net.azurewebsites.telesom",
      "versionCode": 1
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
